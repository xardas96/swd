<Window x:Class="SWD_projekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="800" ResizeMode="CanResize" SizeToContent="Manual" WindowStartupLocation="CenterScreen" Background="#FFE8E6E6">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Frame Name="contentFrame" NavigationUIVisibility="Hidden" />
        <Button Content="{StaticResource back}" Grid.Row="1" Margin="12,0,0,7" Name="backButton" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="75" IsEnabled="False" Click="backButton_Click"/>
        <Button Content="{StaticResource next}" Grid.Row="1" Margin="0,0,12,7" Name="nextButton" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="nextButton_Click" />
    </Grid>
</Window>
